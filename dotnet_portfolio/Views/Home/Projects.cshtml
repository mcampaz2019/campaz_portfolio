    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Portfolio Contact</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="">
    </head>
    <body>
        <div>

        </div>
        <div>
            @Html.Partial("Navigation");
        </div>
        <!--[if lt IE 7]>
            <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="#">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
        <div class=" row justify-content-start"><div class="col-1"></div><h1>As Current time and date:</h1></div>
        <div class=" row justify-content-start "><div class="col-1"></div><div class="alert-success col-4">@myDate</div></div>
        <div class=" row justify-content-center"><h1>My Current Projects!</h1></div>
        <div class="row" id="projectBox">
            <div class="col-1"></div>
            <div class="col-4">
                <div class="list-group" id="myList" role="tablist">
                    @{
                        foreach(KeyValuePair<string, string> project in ListOfMyProjects)
                        {     
                            var name = (project.Key).Split(" ")[0];                 
                            <a class="list-group-item list-group-item-action" data-toggle="list" 
                                href="#@name" role="tab" aria-controls="">@project.Key</a>
                        }
                    }
                
            </div>
            </div>
            <div class="col-7">
                <div class="tab-content alert alert-primary" id="nav-tabContent">
                     @{
                        foreach(KeyValuePair<string, string> project in ListOfMyProjects)
                        {   
                            var name = (project.Key).Split(" ")[0];
                            <div class="tab-pane fade show" id="@name" role="tabpanel" aria-labelledby="listID">@project.Value</div>
                        }
                    }            
                </div>
            </div>
        </div>
        <div>
            @Html.Partial("FooterScript");
        </div>
        <script>        
            $('#myList a:first-child').tab('show')
            $('#myList a').on('click', function (e) {
                e.preventDefault()
                $(this).tab('show')
            })
        </script>
    </body>
</html>